{
    "name": "rgasch/twitter-client",
    "description": "PHP Client the Twitter v2 API",
    "keywords": [
         "Twitter", "API", "Client", "PHP"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/rgasch/twitter-client",
    "authors": [{
        "name": "Robert Gasch",
        "email": "rgasch@gmail.com"
    }],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.5",
        "illuminate/support": "^9.0|^10.0"
    },
    "require-dev": {
        "nunomaduro/larastan": "^2.2",
        "nunomaduro/phpinsights": "dev-master",
        "pestphp/pest": "^1.16",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.5.10",
        "rector/rector": "^0.14.6",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Rgasch\\TwitterClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
